{
  "name": "mern-shopping-cart",
  "version": "1.0.0",
  "description": "shopping list built with mern stack",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CodyFrank/MERN-shopping-cart.git"
  },
  "author": "Cody Frank",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CodyFrank/MERN-shopping-cart/issues"
  },
  "homepage": "https://github.com/CodyFrank/MERN-shopping-cart#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "bootstrap": "^4.5.0",
    "concurrently": "^5.2.0",
    "config": "^3.3.1",
    "cors": "^2.8.5",
    "dot-env": "0.0.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.3",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.21",
    "reactstrap": "^8.5.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.4"
  }
}
